{
	"name": "alpha-global-header",
	"version": "1.1.1",
	"author": "Relentless Technology",
	"license": "ISC",
	"description": "Global Header for Alpha",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/alpha-canada/alpha-global-header.git"
	},
	"publishConfig": {},
	"main": "./dist/angular/umd/index.js",
	"module": "./dist/angular/umd/index.js",
	"files": [
		"dist/angular"
	],
	"typings": "./dist/angular/esm/index.d.ts",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "npm run polymer:start | npm run angular:start",
		"build": "npm run polymer:build && npm run angular:build",
		"polymer:start": "npm run polymer:watch | polymer serve",
		"polymer:watch": "webpack --config src/polymer/webpack.config.polymer.bundle.js --progress --profile --bail --watch",
		"polymer:build": "npm run polymer:build:standalone && npm run polymer:build:bundle",
		"polymer:build:standalone": "webpack --config src/polymer/webpack.config.polymer.js --progress --profile --bail",
		"polymer:build:bundle": "webpack --config src/polymer/webpack.config.polymer.bundle.js --progress --profile --bail",
		"angular:start": "ng serve",
		"angular:build": "npm run angular:build:umd && npm run angular:build:esm",
		"angular:build:umd": "webpack --config src/angular/webpack.config.umd.js --progress --profile --bail",
		"angular:build:esm": "ngc -p src/angular/tsconfig.esm.json",
		"angular:test": "ng test",
		"angular:lint": "ng lint",
		"angular:e2e": "ng e2e"
	},
	"devDependencies": {
		"@angular/animations": "^4.4.6",
		"@angular/cli": "1.4.5",
		"@angular/common": "^4.4.6",
		"@angular/compiler": "^4.4.6",
		"@angular/compiler-cli": "^4.4.6",
		"@angular/core": "^4.4.6",
		"@angular/platform-browser": "^4.4.6",
		"@angular/platform-browser-dynamic": "^4.4.6",
		"@angular/router": "^4.4.6",
		"@types/jasmine": "^2.6.3",
		"@types/jasminewd2": "~2.0.2",
		"@types/node": "^8.0.53",
		"angular2-template-loader": "^0.6.2",
		"autoprefixer": "^7.1.6",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-es2015": "^6.24.1",
		"better-console": "^1.0.1",
		"codelyzer": "^3.2.2",
		"core-js": "^2.4.1",
		"gulp": "*",
		"gulp-rt-font-config": "^1.2.0",
		"jasmine-core": "~2.8.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~1.7.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-cli": "~1.0.1",
		"karma-coverage-istanbul-reporter": "^1.2.1",
		"karma-jasmine": "~1.1.0",
		"karma-jasmine-html-reporter": "^0.2.2",
		"less-plugin-autoprefix": "^1.5.1",
		"polymer-webpack-loader": "^2.0.0",
		"protractor": "~5.1.2",
		"rxjs": "^5.5.2",
		"ts-loader": "^2.3.7",
		"ts-node": "~3.3.0",
		"tslint": "~5.7.0",
		"tslint-loader": "^3.5.3",
		"typescript": "~2.5.3",
		"uglify-js": "git://github.com/mishoo/UglifyJS2.git#v2.8.29",
		"uglifyjs-webpack-plugin": "^1.0.1",
		"webpack": "^3.8.1",
		"webpack-angular-externals": "^1.0.2",
		"webpack-rxjs-externals": "^1.1.0",
		"zone.js": "^0.8.14"
	},
