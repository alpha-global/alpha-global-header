{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "alpha-global-header"
  },
  "apps": [

    {
      "name" : "demo-build",
      "root": "demo/angular/src",
      "outDir": "dist/angular/demo",

      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.demo.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./demo/angular/protractor.conf.js"
    }
  },
  "lint": [

  ],
  "test": {
    "karma": {
      "config": "./demo/angular/karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
